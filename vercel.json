{
    "version": 2,
    "builds": [
        {
            "src": "*.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/game.js",
            "dest": "/game.js"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}